              "raw": "{\n  \"apiKeyId\": 1,\n  \"keyName\": \"Updated Key Name\",\n  \"description\": \"Updated description\",\n  \"isActive\": true,\n  \"expiresAt\": \"2026-12-31T23:59:59Z\",\n  \"isDeleted\": false,\n  \"createdAt\": \"2025-11-18T00:00:00Z\",\n  \"modifiedAt\": \"2025-11-18T12:00:00Z\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/odata/ApiKeys(1)",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "ApiKeys(1)"]
            }
          }
        }
      ]
