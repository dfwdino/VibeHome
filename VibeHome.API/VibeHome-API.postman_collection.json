{
  "info": {
    "name": "VibeHome API",
    "description": "Collection for testing the VibeHome OData API with API Key authentication",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000",
      "type": "string"
    },
    {
      "key": "apiKey",
      "value": "test-api-key-12345",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Kids",
      "item": [
        {
          "name": "Get All Kids",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/odata/Kids",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids"]
            }
          }
        },
        {
          "name": "Get Kid by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/odata/Kids(1)",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids(1)"]
            }
          }
        },
        {
          "name": "Filter Kids by Name",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/odata/Kids?$filter=contains(Name, 'John')",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids"],
              "query": [
                {
                  "key": "$filter",
                  "value": "contains(Name, 'John')"
                }
              ]
            }
          }
        },
        {
          "name": "Create Kid",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"New Kid\",\n  \"createdAt\": \"2025-11-18T00:00:00Z\",\n  \"modifiedAt\": \"2025-11-18T00:00:00Z\",\n  \"isDeleted\": false\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/odata/Kids",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids"]
            }
          }
        },
        {
          "name": "Update Kid",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"kidId\": 1,\n  \"name\": \"Updated Kid Name\",\n  \"createdAt\": \"2025-11-18T00:00:00Z\",\n  \"modifiedAt\": \"2025-11-18T12:00:00Z\",\n  \"isDeleted\": false\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/odata/Kids(1)",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids(1)"]
            }
          }
        },
        {
          "name": "Delete Kid",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/odata/Kids(1)",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "Kids(1)"]
            }
          }
        }
      ]
    },
