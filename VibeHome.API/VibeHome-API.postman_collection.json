        },
        {
          "name": "Get Chore Completions with Expand",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/odata/ChoreCompletions?$top=10&$expand=Kid,ChoreItem&$orderby=CompletedAt desc",
              "host": ["{{baseUrl}}"],
              "path": ["odata", "ChoreCompletions"],
              "query": [
                {
                  "key": "$top",
